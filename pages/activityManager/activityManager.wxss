.follow_wrapper .step_item .si_middle .simsb_primary {
  color: #3173cd;
  background: rgba(49, 115, 205, 0.05);
}
.follow_wrapper .step_item .si_bottom {
  margin-top: 22rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 36rpx;
}
.follow_wrapper .step_item .rotate_spot {
  width: 8rpx;
  height: 8rpx;
  background: #3173cd;
  position: absolute;
  top: 34rpx;
  left: -22rpx;
  transform: rotate(45deg);
  box-sizing: border-box;
  margin: 6rpx 0;
  z-index: 10;
}
.follow_wrapper .step_item .botted_line {
  height: 97%;
  position: absolute;
  top: 56rpx;
  left: -20rpx;
  border-left: 2rpx #e6e7e8 dashed;
}

